{
  "extends": "expo/tsconfig.base",
  "include": ["./index.d.ts", "./src"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "api/": ["./src/api/"],
      "assets/*": ["./assets/*"],
      "components/": ["./src/components"],
      "containers/": ["./src/containers"],
      "helpers/": ["./src/helpers"],
      "hooks/": ["./src/hooks"],
      "navigation/": ["./src/navigation"],
      "screens/": ["./src/screens/"],
      "screens/*": ["./src/screens/*"],
      "store/": ["./src/store"],
      "store/*": ["./src/store/*"],
      "theme/": ["./src/theme"]
    }
  }
}
